{
  "mcpServers": {
    "nuxt": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://nuxt.com/mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "nuxt-ui": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://ui.nuxt.com/mcp"
      ],
      "env": {}
    },
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "mcp",
        "--dir",
        ".",
        "--only",
        "storage"
      ],
      "env": {},
      "autoApprove": [
        "storage_get_object_download_url"
      ]
    }
  }
}